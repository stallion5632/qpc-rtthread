import rtconfig
from building import *

cwd = GetCurrentDir()

# QXK Demo source files
src = Split("""
main.c
rt_integration.c
""")

# Include paths
path = [cwd]

# Define the QXK demo group
# This group depends on QPC with QXK kernel support
group = DefineGroup('qpc_qxk_demo', src, 
                   depend = ['PKG_USING_QPC', 'QPC_USING_QXK_DEMO', 'RT_USING_FINSH'], 
                   CPPPATH = path)

Return('group')